<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shafin.shareride">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SigninActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".MembersActivity"
            android:label="@string/title_activity_members"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PostActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".FAQActivity" />
        <activity android:name=".ComplainActivity" />
        <activity android:name=".FindRideActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".ToolActivity" />
        <activity android:name=".SearchActivity"></activity>
    </application>

</manifest>